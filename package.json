{
  "name": "webpack-seal-analyzer",
  "version": "1.0.0",
  "description": "A webpack plugin that analyzes and visualizes the relationships between modules, chunks, and module graphs during the seal phase",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "lint": "eslint \"src/**/*.{js,ts}\" \"test/**/*.{js,ts}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts}\" \"test/**/*.{js,ts}\" --fix",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install && npm run build",
    "pre-commit": "lint-staged",
    "example": "cd example && npm install && npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "webpack",
    "plugin",
    "analyzer",
    "modules",
    "chunks",
    "dependencies"
  ],
  "author": "DXM666 (https://github.com/DXM666)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DXM666/webpack-seal-analyzer"
  },
  "bugs": {
    "url": "https://github.com/DXM666/webpack-seal-analyzer/issues"
  },
  "homepage": "https://github.com/DXM666/webpack-seal-analyzer#readme",
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^8.0.3",
    "jest": "^29.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
